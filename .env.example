# File: .env.example
# 백엔드 - 데이터베이스
SPRING_DATASOURCE_URL=jdbc:mysql://your-db-host:3306/myson_db?useSSL=false&useUnicode=true&characterEncoding=utf8&serverTimezone=Asia/Seoul&allowPublicKeyRetrieval=true
SPRING_DATASOURCE_USERNAME=your_username
SPRING_DATASOURCE_PASSWORD=your_password

# 백엔드 - API & 보안
API_SERVICEKEY=your_api_key
JWT_SECRET=your_jwt_secret

# 백엔드 - Google OAuth2
SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_GOOGLE_CLIENT_ID=your_client_id
SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_GOOGLE_CLIENT_SECRET=your_client_secret
SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_GOOGLE_SCOPE=profile,email

# 백엔드 - CORS
APP_CORS_ALLOWED_ORIGINS=http://localhost:5173

# 프론트엔드 - API
VITE_API_URL=http://your-server-ip:8080
VITE_API_BASE_URL=http://localhost:8080

# 프론트엔드 - Kakao
VITE_KAKAO_APP_KEY=your_kakao_app_key

